package com.pcs.ztqtj.view.myview;

import android.content.Context;
import android.graphics.drawable.Drawable;
import android.util.AttributeSet;
import android.widget.SeekBar;

/** 
 * 自定义的SeekBar
 * @author chenjh
 */
public class MySeekBarThumb extends SeekBar {

	Drawable mThumb;

	public MySeekBarThumb(Context context) {
		super(context);
	}

	public MySeekBarThumb(Context context, AttributeSet attrs) {
		super(context, attrs);
	}

	public MySeekBarThumb(Context context, AttributeSet attrs, int defStyle) {
		super(context, attrs, defStyle);
	}

	@Override
	public void setThumb(Drawable thumb) {
		super.setThumb(thumb);
		mThumb = thumb;
	}

	public Drawable getSeekBarThumb() {
		return mThumb;
	}

}
