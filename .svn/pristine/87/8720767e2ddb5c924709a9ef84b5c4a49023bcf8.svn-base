package com.pcs.lib_ztqfj_v2.model.pack.net;

import com.pcs.lib.lib_pcs_v3.model.pack.PcsPackUp;

import org.json.JSONException;
import org.json.JSONObject;

/**
 * 气象影视吐槽上传接口
 */
public class PackMediaTauntedListUp extends PcsPackUp {
    public static final String NAME = "wt_v_suggest_list";
    public int count = 20;
    public String channel_id;

    public PackMediaTauntedListUp() {
        intervalMill = 0;
    }

    @Override
    public JSONObject toJSONObject() {
        try {
            JSONObject obj = new JSONObject();
            obj.put("count", count);
            obj.put("channel_id", channel_id);
            return obj;
        } catch (JSONException e) {
            e.printStackTrace();
        }
        return null;
    }

    @Override
    public String getName() {
        return NAME;
    }
}
