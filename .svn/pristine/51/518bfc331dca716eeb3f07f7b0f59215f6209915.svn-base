package com.pcs.lib_ztqfj_v2.model.pack.net.livequery;

import com.pcs.lib.lib_pcs_v3.model.pack.PcsPackUp;

import org.json.JSONObject;


/**
 * 地区半年降雨量对比
 */
public class PackRainstatNewUp extends PcsPackUp {
    public static final String NAME = "rainstat_min";
    public String city;
    public String county;
    public String begtime;
    public String endtime;
    public String datatype;

    public PackRainstatNewUp() {
        intervalMill = 0;
    }

    @Override
    public JSONObject toJSONObject() {
        try {
            JSONObject obj = new JSONObject();
            obj.put("city", city);
            obj.put("county", county);
            obj.put("datatype", datatype);
            obj.put("begtime", begtime);
            obj.put("endtime", endtime);
            return obj;
        } catch (Exception e) {
            e.printStackTrace();
        }
        return null;
    }

    @Override
    public String getName() {
        return NAME + "#" + city + "_" + county;
    }
}
