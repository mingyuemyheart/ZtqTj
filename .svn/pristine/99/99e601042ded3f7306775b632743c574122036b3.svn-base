package com.pcs.lib_ztqfj_v2.model.pack.net;

import com.pcs.lib.lib_pcs_v3.model.pack.PcsPackDown;

import org.json.JSONArray;
import org.json.JSONException;
import org.json.JSONObject;

import java.util.ArrayList;
import java.util.List;


/**
 * 查询推送
 * 
 */
public class PackPushWeatherConfigDown extends PcsPackDown {
    public List<PushWeatherBean> list = new ArrayList<>();

	@Override
	public void fillData(String jsonStr) {
        list.clear();
		try {
			JSONObject obj = new JSONObject(jsonStr);
			updateMill = obj.optLong("updateMill");
			JSONArray areasArray = obj.getJSONArray("info_list");
            for(int i = 0; i < areasArray.length(); i++) {
                JSONObject jsonObject = areasArray.getJSONObject(i);
                PushWeatherBean bean = new PushWeatherBean();
                bean.id = jsonObject.getString("id");
                bean.tag = jsonObject.getString("tag");
                bean.memo = jsonObject.getString("memo");
                bean.s_memo = jsonObject.getString("s_memo");
                list.add(bean);
            }
		} catch (JSONException e) {
			e.printStackTrace();
		}
	}

	@Override
	public String toString() {
		return null;
	}

	public class PushWeatherBean {
        public String id = "";//ID
        public String tag = "";//标识
        public String memo = "";// 备注
        public String s_memo = "";// 推送备注
    }

}
