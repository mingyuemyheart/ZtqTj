package com.pcs.lib_ztqfj_v2.model.pack.net;

import android.text.TextUtils;

import com.pcs.lib.lib_pcs_v3.model.pack.PcsPackDown;

import org.json.JSONArray;
import org.json.JSONException;
import org.json.JSONObject;

import java.util.ArrayList;
import java.util.List;


public class PackImWeatherDown extends PcsPackDown {
	public List<ImWeather> arrayList = new ArrayList<>();

	@Override
	public void fillData(String jsonStr) {
        arrayList.clear();
		if (TextUtils.isEmpty(jsonStr)) {
			return;
		}
		JSONObject temp;
		try {
			temp = new JSONObject(jsonStr);
			updateMill = temp.optLong("updateMill");
			JSONArray arr = temp.getJSONArray("info_list");
			for (int i = 0; i < arr.length(); i++) {
                ImWeather imWeather = new ImWeather();
				JSONObject airJson = arr.getJSONObject(i);
                imWeather.date = airJson.optString("fb_time");
                imWeather.title = airJson.optString("title");
                imWeather.url=airJson.optString("html_path");
                arrayList.add(imWeather);
			}

		} catch (JSONException e) {
			e.printStackTrace();
		}
	}

	public class ImWeather{
        public String title;
        public String date;
        public String url;
    }
}
