package com.pcs.lib_ztqfj_v2.model.pack.net.week;

/**
 * 一周天气
 *
 * @author JiangZy
 */
public class PackWeekWeatherFamilyUp extends PackWeekWeatherUp {
    public static final String NAME = "new_week_s_y";
    public PackWeekWeatherFamilyUp() {
        // 间隔毫秒数
        intervalMill = 0;
        // 循环发送
        bRepeat = false;
    }
    public void setCityId(String cityId) {
        area = cityId;
    }

    @Override
    public String getName() {
        return NAME + "#" + area +"_"+ country;
    }
}
