package com.pcs.lib_ztqfj_v2.model.pack.net.expert;

import com.pcs.lib.lib_pcs_v3.model.pack.PcsPackUp;

import org.json.JSONException;
import org.json.JSONObject;


/**
 * 天气解读
 *
 * @author chenjh
 */
public class PackExpertDetailTalkCommitUp extends PcsPackUp {
    public static final String NAME = "tq_zx_pl_tj";
    public String zx_id;
    public String user_id;
    public String content;

    public PackExpertDetailTalkCommitUp() {
        intervalMill = 0;
        bRepeat = false;
    }

    @Override
    public JSONObject toJSONObject() {
        try {
            JSONObject obj = new JSONObject();
            obj.put("zx_id", zx_id);
            obj.put("user_id", user_id);
            obj.put("content", content);
            return obj;
        } catch (JSONException e) {
            e.printStackTrace();
        }
        return null;
    }

    @Override
    public String getName() {
        return NAME + "#" + zx_id + "_" + user_id;
    }
}