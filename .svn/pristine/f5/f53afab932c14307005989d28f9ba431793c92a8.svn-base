package com.pcs.lib_ztqfj_v2.model.pack.net;

import com.pcs.lib.lib_pcs_v3.model.pack.PcsPackUp;

import org.json.JSONException;
import org.json.JSONObject;


/**
 * 意见列表上传包
 *
 * @author Chensq
 */
public class PackSuggestListUp extends PcsPackUp {

    public static final String NAME = "suggest_list";

    public String count = "";//意见条数

    public PackSuggestListUp() {
        intervalMill = 0;
    }

    @Override
    public JSONObject toJSONObject() {
        try {
            JSONObject obj = new JSONObject();
            obj.put("count", count);
            return obj;
        } catch (JSONException e) {
            e.printStackTrace();
        }
        return null;
    }

    @Override
    public String getName() {
        return NAME;
    }

}
