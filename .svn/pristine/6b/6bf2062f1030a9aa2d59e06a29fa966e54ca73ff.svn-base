package com.pcs.lib_ztqfj_v2.model.pack.net.service;

import com.pcs.lib.lib_pcs_v3.model.pack.PcsPackUp;

import org.json.JSONException;
import org.json.JSONObject;

/**
 * 气象服务-登录查看上传包
 * Created by tyaathome on 2016/8/24.
 */
public class PackServiceLoginQueryUp extends PcsPackUp {

    public static final String NAME = "qxfw_login_query";

    public String user_id = "";

    public PackServiceLoginQueryUp() {
        //this.intervalMill = 60 * 1000 * 1;
        this.intervalMill = 0;
    }

    @Override
    public JSONObject toJSONObject() {
        JSONObject obj = new JSONObject();
        try {
            obj.put("user_id", user_id);
        } catch (JSONException e) {
            e.printStackTrace();
        }
        return obj;
    }

    @Override
    public String getName() {
        //return NAME + "#" + user_id;
        return NAME;
    }
}
