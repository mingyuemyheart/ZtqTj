package com.pcs.lib_ztqfj_v2.model.pack.net.warn;

import com.pcs.lib_ztqfj_v2.model.pack.local.PackLocalCity;
import com.pcs.lib_ztqfj_v2.model.pack.net.PackShortUpdateTimeUp;

import org.json.JSONException;
import org.json.JSONObject;


/**
 * 首页预警信息
 *
 * @author JiangZy
 */
public class PackYjxxIndexUp extends PackShortUpdateTimeUp {

    public static final String NAME = "yjxx_index_fb";

    private String area = "";

    public PackYjxxIndexUp() {
        // 循环发送
        //bRepeat = true;
    }


    public void setCity(PackLocalCity cityInfo){
        if(cityInfo.isFjCity){
            area = cityInfo.PARENT_ID;
        }else{
            area = cityInfo.ID;
        }
    }

    @Override
    public JSONObject toJSONObject() {
        try {
            JSONObject obj = new JSONObject();
            obj.put("area", area);

            return obj;
        } catch (JSONException e) {
            e.printStackTrace();
        }
        return null;
    }

    /**
     * 获取接口名
     *
     * @return
     */
    @Override
    public String getName() {
        return NAME + "#" + area;
    }
}