<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/item_fragmentset_id"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingTop="@dimen/dimen8"
    android:paddingBottom="@dimen/dimen8"
                android:paddingLeft="@dimen/dimen10"
                android:paddingRight="@dimen/dimen10"
     >

    <ImageView
        android:id="@+id/setimage_icon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
         />

    <CheckBox
        android:id="@+id/open_icon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:background="@drawable/checkbox_commom"
        android:button="@null"
        android:focusable="false"
        android:visibility="gone"/>

    <TextView
        android:id="@+id/tv_right"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:textColor="@color/text_black_login"
        android:textSize="@dimen/text_footnote"
        android:visibility="gone"/>

    <LinearLayout 
        android:layout_width="match_parent"
        android:layout_height="match_parent"
       	android:layout_toLeftOf="@id/open_icon"
       	android:layout_toRightOf="@id/setimage_icon"
        android:layout_centerVertical="true">
	    <TextView
	        android:id="@+id/explain_text"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_gravity="bottom"
	        android:textSize="@dimen/text_content"
	        android:textColor="@color/item_textcolor"
	        android:layout_marginLeft="@dimen/activity_margintop"
	    />
	    
	    <TextView 
	        android:id="@+id/sub_text"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_gravity="bottom"
	        android:textSize="@dimen/text_footnote"
	        android:textColor="@color/text_gray_light"
	        android:layout_marginLeft="@dimen/activity_margintop"
	        />
    </LinearLayout>

</RelativeLayout>