package com.pcs.lib_ztqfj_v2.model.pack.net;

import org.json.JSONException;
import org.json.JSONObject;



/**
 * 生活指数
 * 
 * @author JiangZy
 * 
 */
public class PackLifeNumberUp extends PackShortUpdateTimeUp {

    public static final String NAME = "shzs";
	public String area;
	public PackLifeNumberUp() {
		// 间隔毫秒数
		//intervalMill = 1000 * 60 * 10;
		// 循环发送
		//bRepeat = true;
	}
	@Override
	public JSONObject toJSONObject() {
		try {
			JSONObject obj = new JSONObject();
			obj.put("area", area);
			return obj;
		} catch (JSONException e) {
			e.printStackTrace();
		}
		return null;
	}

	/**
	 * 获取接口名
	 * 
	 * @return
	 */
	@Override
	public String getName() {
		return NAME + "#" + area;
	}
}