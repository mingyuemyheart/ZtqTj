package com.pcs.lib_ztqfj_v2.model.pack.net.warn;

import android.os.Parcel;
import android.os.Parcelable;

import java.io.Serializable;

/**
 * Created by Administrator on 2017/7/14 0014.
 * chen_jx
 */

public class YjxxInfo implements Serializable, Parcelable{

    /**
     *
     */
    public String id;
    /**
     * 文字
     */
    public String desc;
    /**
     * 图标
     */
    public String ico;
    /**
     * 内容
     */
    public String content;

    /**
     * 预警类型
     * @param jsonStr
     */
    public String yj_type;

    public YjxxInfo() {

    }


    protected YjxxInfo(Parcel in) {
        id = in.readString();
        desc = in.readString();
        ico = in.readString();
        content = in.readString();
        yj_type = in.readString();
    }

    public static final Creator<YjxxInfo> CREATOR = new Creator<YjxxInfo>() {
        @Override
        public YjxxInfo createFromParcel(Parcel in) {
            return new YjxxInfo(in);
        }

        @Override
        public YjxxInfo[] newArray(int size) {
            return new YjxxInfo[size];
        }
    };

    @Override
    public int describeContents() {
        return 0;
    }

    @Override
    public void writeToParcel(Parcel dest, int flags) {
        dest.writeString(id);
        dest.writeString(desc);
        dest.writeString(ico);
        dest.writeString(content);
        dest.writeString(yj_type);
    }
}
