package com.pcs.lib_ztqfj_v2.model.pack.net.livequery;

import com.pcs.lib.lib_pcs_v3.model.pack.PcsPackDown;

import org.json.JSONArray;
import org.json.JSONException;
import org.json.JSONObject;

import java.util.ArrayList;
import java.util.List;



/**
 *福建省雨量分布图
 */
public class PackYltjImgDown extends PcsPackDown {
	public List<YltjImg> datalist;
	@Override
	public void fillData(String jsonStr) {
		datalist=new ArrayList<YltjImg>();
		JSONObject temp;
		try {
			temp = new JSONObject(jsonStr);
			updateMill = temp.optLong("updateMill");
			JSONArray arr = temp.getJSONArray("datalist");
			for (int i = 0; i < arr.length(); i++) {
				JSONObject obj=arr.getJSONObject(i);
				YltjImg imgobject=new YltjImg();
				imgobject.channel_name=obj.getString("channel_name");
				imgobject.img=obj.getString("img");
				datalist.add(imgobject);
			}
		} catch (JSONException e) {
			e.printStackTrace();
		}
	}

	@Override
	public String toString() {
		return null;
	}
	
	public class YltjImg{
		public String channel_name;
		public String img;
	}
	
}
