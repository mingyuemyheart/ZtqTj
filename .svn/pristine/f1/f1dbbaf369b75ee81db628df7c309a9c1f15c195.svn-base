package com.pcs.lib_ztqfj_v2.model.pack.net.waterflood;

import com.pcs.lib.lib_pcs_v3.model.pack.PcsPackUp;

import org.json.JSONException;
import org.json.JSONObject;



/**
 * 雨晴信息-站点的单时次雨量信息上传包
 * @author tya
 *
 */
public class PackRainInfoStationOneUp extends PcsPackUp {

    public static final String NAME = "fx_station_one_raininfo";

	/**
	 * 查询时间
	 */
	public String time = "";
	
	/**
	 * 站点ID
	 */
	public String station_id = "";

	
	public PackRainInfoStationOneUp() {
		intervalMill = 0;
	}
	
	@Override
	public JSONObject toJSONObject() {
		JSONObject obj = new JSONObject();
		try {
			obj.put("time", time);
			obj.put("station_id", station_id);
		} catch (JSONException e) {
			e.printStackTrace();
		}
		return obj;
	}

	@Override
	public String getName() {
		return NAME;
	}

}
