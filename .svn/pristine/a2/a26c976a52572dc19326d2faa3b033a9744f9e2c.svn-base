package com.pcs.lib_ztqfj_v2.model.pack.net.radar;

import com.pcs.lib.lib_pcs_v3.model.pack.PcsPackDown;

import org.json.JSONArray;
import org.json.JSONException;
import org.json.JSONObject;

import java.util.ArrayList;
import java.util.List;


/**
 * 气象雷达
 * 
 * @author chenjh
 * 
 */
public class PackRadarDown extends PcsPackDown {

	public List<RadarImgInfo> radarImgList = new ArrayList<RadarImgInfo>();

	@Override
	public void fillData(String jsonStr) {
		JSONObject temp;
		try {
			radarImgList.clear();
			temp = new JSONObject(jsonStr);
			updateMill = temp.optLong("updateMill");
			JSONArray arr = temp.getJSONArray("idex");

			for (int i = 0; i < arr.length(); i++) {
				RadarImgInfo info = new RadarImgInfo();
				JSONObject airJson = arr.getJSONObject(i);
				info.actiontime = airJson.getString("actiontime");
				info.img = airJson.getString("img");
				radarImgList.add(info);
			}

		} catch (JSONException e) {
			e.printStackTrace();
		}
	}

	@Override
	public String toString() {

		return null;
	}

}
