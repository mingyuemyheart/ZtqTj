package com.pcs.lib_ztqfj_v2.model.pack.net.waterflood;

import com.pcs.lib.lib_pcs_v3.model.pack.PcsPackUp;

import org.json.JSONException;
import org.json.JSONObject;



/**
 * 雨晴信息-所有站点的单时次雨量信息上传包
 * @author tya
 *
 */
public class PackRainInfoOneUp extends PcsPackUp {

    public static final String NAME = "fx_one_raininfo";

	/**
	 * 查询时间
	 */
	public String time = "";
	
	/**
	 * 排序(默认2为升序;1为降序 )
	 */
	public String rank = "";
	
	public PackRainInfoOneUp() {
		intervalMill = 0;
	}
	
	@Override
	public JSONObject toJSONObject() {
		JSONObject obj = new JSONObject();
		try {
			obj.put("time", time);
			obj.put("rank", rank);
		} catch (JSONException e) {
			e.printStackTrace();
		}
		return obj;
	}

	@Override
	public String getName() {
		return NAME;
	}

}
