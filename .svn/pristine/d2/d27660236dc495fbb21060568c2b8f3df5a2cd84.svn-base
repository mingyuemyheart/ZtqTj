package com.pcs.lib_ztqfj_v2.model.pack.net.warn.sh_warn;

import com.pcs.lib.lib_pcs_v3.model.pack.PcsPackDown;

import org.json.JSONArray;
import org.json.JSONException;
import org.json.JSONObject;

import java.util.ArrayList;
import java.util.List;

/**
 * Created by Administrator on 2017/7/14 0014.
 * chen_jx
 */

public class PackYjDepDown extends PcsPackDown {

    public List<YjZqInfoAreaDown> list_2=new ArrayList<>();
    @Override
    public void fillData(String jsonStr) {

        list_2.clear();
        try {
            JSONObject obj = new JSONObject(jsonStr);
            updateMill = obj.optLong("updateMill");
            JSONArray jsonArraydz = obj.getJSONArray("info_list");
            if (jsonArraydz!=null) {
                for (int i = 0; i < jsonArraydz.length(); i++) {
                    YjZqInfoAreaDown yjZqInfoArea = new YjZqInfoAreaDown();
                    yjZqInfoArea.s_id = jsonArraydz.getJSONObject(i).getString("town_id");
                    yjZqInfoArea.s_name = jsonArraydz.getJSONObject(i).getString("town_name");

                    list_2.add(yjZqInfoArea);
                }
            }
        } catch (JSONException e) {
            e.printStackTrace();
        }
    }
}
